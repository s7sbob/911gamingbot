# Copy this file to .env and fill in your credentials

# Discord bot token obtained from Discord Developer Portal
DISCORD_TOKEN=YOUR_BOT_TOKEN

# MongoDB connection string (e.g., mongodb+srv://user:password@cluster.mongodb.net/db)
# Leave blank to use in-memory storage for testing
MONGODB_URI=

# URL of the image to use as the bot's avatar (must be an accessible URL). If unset, the current avatar is kept.
BOT_AVATAR_URL=https://example.com/your-logo.png

# Role ID that will be granted to users who verify using the verify button
VERIFICATION_ROLE_ID=123456789012345678

# Role ID allowed to view and respond to ticket channels (support staff)
SUPPORT_ROLE_ID=123456789012345678

# Category ID where ticket channels should be created
TICKET_CATEGORY_ID=123456789012345678

# Channel ID to send moderation logs (kick, ban, mute)
MODLOG_CHANNEL_ID=123456789012345678

# Channel ID where advertisements should be posted when admins use /ad
AD_CHANNEL_ID=123456789012345678

# Channel ID where ticket transcripts should be sent
TRANSCRIPT_CHANNEL_ID=123456789012345678

# Channel IDs used for anonymous announcements. When an admin sends a
# message in ANNOUNCEMENT_SOURCE_CHANNEL_ID, the bot will delete the
# original message and repost it in ANNOUNCEMENT_CHANNEL_ID under the
# bot's identity. This allows administrators to make announcements
# without revealing their username.
ANNOUNCEMENT_SOURCE_CHANNEL_ID=123456789012345678
ANNOUNCEMENT_CHANNEL_ID=123456789012345678

# Default volume for the music player (0â€“100)
MUSIC_VOLUME=50

# OAuth2 configuration for adding users to your guild via the guilds.join scope.
# These values are required if you plan to use the joinlink and joincomplete commands.
# OAUTH_CLIENT_ID: the Client ID of your Discord application
OAUTH_CLIENT_ID=1472762695282458664
# OAUTH_CLIENT_SECRET: the Client Secret of your Discord application
OAUTH_CLIENT_SECRET=hEQFOV2ghYaeZgjFsBFrXALoN5sGcAiJ
# OAUTH_REDIRECT_URI: the redirect URI registered in your application settings
OAUTH_REDIRECT_URI=https://www.911gamingstore.com
# TARGET_GUILD_ID: the ID of the guild to add authorised users to
TARGET_GUILD_ID=681663750692143124